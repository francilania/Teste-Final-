package testefinal;

public class Veiculo {

   private String placa;
   private String cor;
   private int numeropassageiros;
   private String capacidadedotanque;
   private double velocidademaxima;
   private double velocidadeatual;
   private String consumomedio;
   
   public void setPlaca (String numeroPlaca) {

  this.placa = numeroPlaca;

  }

  public String getPlaca( ) {

  return this.placa;

  }
  
  public void setCor (String veiculoCor) {

this.cor = veiculoCor;

}

public String getCor ( ) {

return this.cor;

}

public void setPassageiros (int numeroPassageiros) {

this.numeropassageiros = numeroPassageiros;

}



Public int getPassageiros ( ) {

return this.numeropassageiros;

}

public void setCapacidade (String capacidadeTanque) {

this.capacidadedotanque = capacidadeTanque;

}

public String getCapacidade ( ) {

return this.capacidadedotanque;

}

public void set Velocidadex ( double velocidadeMaxima ) {

this.velocidademaxima = velocidadeMaxima;

}

public double getVelocidadex  ( ) {

return this.velocidademaxima;

}

public void setVelocidadea (double velocidadeAtual ) {

this.velocidadeatual = velocidadeAtual;

}

public double getVelocidadea ( ) {

return this.velocidadeatual;

}

public void setConsumo (String consumoMedio) {

this.consumomedio = consumoMedio;

}

public String getConsumo ( ) {

return this.consumomedio;

}

package testefinal;

public class Livro {

private String titulo;
private String autor;
private String isbn;
private int numerodepaginas;
private double valordecompra;

public void setTitulo (String livroTitulo) {

this.titulo = livroTitulo;

}

public String getTitulo ( ) {

return this.titulo;

}

public void setAutor (String livroAutor) {

this.autor = livroAutor;

}
 public String getAutor ( ) {

return this.autor;

}

public void setIsbn (String livroIsbn) {

this.isbn = livroIsbn;

}

public String getIsbn ( ) {

return this.isbn;

}
public void setPaginas ( int numeroPaginas ) {

this.numerodepaginas = numeroPaginas;

}

public int getPaginas ( ) {

return this.numerodepaginas;

}

public void setCompra ( double valorCompra ) {

this.valordecompra = valorCompra;

}

public doubleCompra ( ) {

return this.valordecompra;

}

}

package testefinal;

public class contaBancaria {

private int numerodaconta;
private String banco;
private int agencia;
private String tipodaconta;
private double saldoatual;
private double limitedisponivel;

public void setConta ( int numeroConta ) {

this.numerodaconta = numeroConta;

}

public int getConta ( ) {

return this.numerodaconta;

}

public void setBanco ( String nomeBanco) {

this.banco = nomeBanco;

}

public String getBanco ( ) {

return this.banco;

}

public void setAgencia (int numeroAgencia) {

this.agencia = numeroAgencia;

}

public int getAgencia ( ) {

return this.agencia; 
}

public void setTipo (String tipoConta) {

this.tipodaconta = tipoConta;

}

public String getTipo ( ) {

return this.tipodaconta;

}

public void setSaldo ( double saldoAtual ) {

this.saldoatual = saldoAtual; 

}

public double getSaldo ( ) {

return this.saldoatual;

}

public void setLimitedisp ( double limiteDisponivel ) {

this.limitedisponivel = limiteDisponivel;

}

public double getLimitedisp ( ) {

return this.limitedisponivel;

}

}

